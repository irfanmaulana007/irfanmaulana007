{
    "extends": "tsconfig/nextjs.json",
    "compilerOptions": {
        "paths": {
            "app/*": ["./app/*"],
            "assets/*": ["./assets/*"],
            "components/*": ["./components/*"],
            "core/*": ["./core/*"],
            "features/*": ["./features/*"],
            "packages/ui": ["./../../packages/ui"],
            "packages/*": ["./../../packages/*"],
            "utils/*": ["./utils/*"],
            "public/*": ["./public/*"],
            "styles/*": ["./styles/*"]
        }
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
    "exclude": ["node_modules"]
}
